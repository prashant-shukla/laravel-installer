    {
    "name": "pacificsw/laravel-installer",
    "description": "Laravel web Installation wizard",
    "keywords": [
        "pacificsw",
        "laravel-installer"
    ],
    "homepage": "https://github.com/prashant-shukla/laravel-installer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Prashant Shukla",
            "email": "prashant.jod@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*" 
    },
    "autoload": {
        "psr-4": {
            "PacificSw\\LaravelInstaller\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PacificSw\\LaravelInstaller\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PacificSw\\LaravelInstaller\\LaravelInstallerServiceProvider"
            ],
            "aliases": {
                "LaravelInstaller": "PacificSw\\LaravelInstaller\\LaravelInstallerFacade"
            }
        }
    }
}
